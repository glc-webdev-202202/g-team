<!DOCTYPE HTML>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Agency Rank</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">

</head>

<header> 
    <%- message %>
    <% if(!loggedin){%>
    <div class="container">
        <div class="row">
            <div class="col-md-6"></div>
            <div class="col-1"><a href="/allstock_data">종목 가격</a></div>
            <div class="col-1"><a href="/agency_rank">기관 수급</a></div>
            <div class="col-1"><a href="/foreigner_rank">외인 수급</a></div>
            <div class="col-1"><a href="/Forum">Forum</a></div>
            <div class="col-1"><a href="/register">Register</a></div>
            <div class="col-1"><a href="/login">Login</a></div> 
        </div>
    </div>
    <%} else {%>
    <div class="container">
        <div class="row">
            <div class="col-md-6"></div>
            <div class="col-1"><a href="/allstock_data">종목 가격</a></div>
            <div class="col-1"><a href="/agency_rank">기관 수급</a></div>
            <div class="col-1"><a href="/foreigner_rank">외인 수급</a></div>
            <div class="col-1"><a href="/Forum">Forum</a></div>
            <div class="col-1"><a href="/profile">Profile</a></div>
            <div class="col-1"><a href="/logout">Logout</a></div> 
        </div>
    </div>
    <%} %>
</header>


<body>
    <div class="container">
        <h1><a href="/">Agency Rank</a></h1>
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>Code</th>
                    <th>Name</th>
                    <th>D</th>
                    <th>D-1</th>
                    <th>D-2</th>
                    <th>D-3</th>
                    <th>D-4</th>
                </tr>
            </thead>
            <tbody>
                <% for(const stock of stocks){ %> <!-- [const name].[*DATABASE COLUMN NAME*, not the name in index.ts] -->
                <tr>
                    <td><%= stock.code %></td>
                    <td><%= stock.name %></td>
                    <td><%= stock.d %></td>
                    <td><%= stock.d1 %></td>
                    <td><%= stock.d2 %></td>
                    <td><%= stock.d3 %></td>
                    <td><%= stock.d4 %></td>
                </tr>
                <% } %>
            </tbody>
        </table>
    </div>