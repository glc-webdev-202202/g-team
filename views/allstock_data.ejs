<body>
    <div class="container">
        <h1 class="secondt text-center"><a href="/">Stocks</a></h1>
        <form action="/search" method="get">
            <div class="input-group mb-3">
                <input type="text" class="form-control" placeholder="Enter stock name" name="stock_name">
                <button class="btn btn-outline-secondary" type="submit" value="submit">Search</button>
            </div>
        </form>
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>Code</th>
                    <th>Name</th>
                    <th>Price</th>
                    <th>Start</th>
                    <th>High</th>
                    <th>Low</th>
                    <th>Volume</th>
                </tr>
            </thead>
            <tbody>
                <% for(const stock of stocks){ %> <!-- [const name].[*DATABASE COLUMN NAME*, not the name in index.ts] -->
                <tr>
                    <td><%= stock.stock_code %></td>
                    <td><%= stock.stock_name %></td>
                    <td><%= stock.stock_price %></td>
                    <td><%= stock.stock_start %></td>
                    <td><%= stock.stock_high %></td>
                    <td><%= stock.stock_low %></td>
                    <td><%= stock.stock_volume %></td> 
                    <td><form action="/favStock" method="post">
                          <input type="hidden" name="stock_code" value="<%= stock.stock_code %>">
                          <button type="submit" class="btn btn-primary">Add to Fav</button>
                      </form></td>
                </tr>
                <% } %>
            </tbody>
        </table>
    </div>

    <%- include('foot') -%>